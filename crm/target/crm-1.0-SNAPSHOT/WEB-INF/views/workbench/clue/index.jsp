<script type="text/javascript">

	$(function(){
		// "创建" 线索按钮
		$("#createButton").click(function (){
			//模态窗口下拉框数据
			// $("#create-clueOwner").empty();
			$.ajax({
				url:"workbench/user",
				data:"",
				type:"GET",
				dataType:"json",
				success:function (data) {
					var $html = "";
					$.each(data,function (i,user){
						$html += "<option value='" + user.id + "'>"+ user.name+ "</option>";
					});

					$("#create-clueOwner").html($html);
					//下拉框默认选中登录者数据
					var id = "${sessionScope.user.id}";
					$("#create-clueOwner").val(id);
				}
			})

			//模态窗口日期选择框
			$("#create-nextContactTime").datetimepicker({
				minView: "month",
				language: 'zh-CN',
				format: 'yyyy-mm-dd',
				autoclose: true,
				todayBtn: true,
				pickerPosition: "bottom-left"
			});

			//show：打开模态窗口，hide：关闭模态窗口
			$("#createClueModal").modal("show");
		});

		//保存线索
		$("#saveClue").click(function (){
			//获取表单中的内容
			$.ajax({
				url:"/workbench/clue",
				data:{
					"owner" : $.trim($("#create-clueOwner").val()),
					"company" : $.trim($("#create-company").val()),
					"appellation" : $.trim($("#create-appellation").val()),
					"fullname" : $.trim($("#create-fullname").val()),
					"job" : $.trim($("#create-job").val()),
					"email" : $.trim($("#create-email").val()),
					"phone" : $.trim($("#create-phone").val()),
					"website" : $.trim($("#create-website").val()),
					"mphone" : $.trim($("#create-mphone").val()),
					"state" : $.trim($("#create-state").val()),
					"source" : $.trim($("#create-source").val()),
					"description" : $.trim($("#create-description").val()),
					"contactSummary" : $.trim($("#create-contactSummary").val()),
					"nextContactTime" : $.trim($("#create-nextContactTime").val()),
					"address" : $.trim($("#create-address").val()),
				},
				type:"POST",
				dataType:"json",
				success:function (data) {
					if (data.flag){
						alert("保存成功");
						//保存完成后清空模态窗口数据
						$("#createClueModalForForm")[0].reset();
						//关闭模态窗口
						$("#createClueModal").modal("hide");

						//刷新页面，应该回到第一页，维持每页展现的记录数
						pageList(1,$("#activityPage").bs_pagination('getOption', 'rowsPerPage'));

					}else{
						alert("保存失败");
					}
				}
			})
		});

		//修改线索按钮
		$("#editButton").click(function (){

		});

		//更新线索按钮
		$("#updatClue").click(function (){

		})

		//删除线索按钮
		$("#deleteClue").click(function (){

		});

		//按条件查询市场活动信息
		$("#selectButton").click(function (){
			//刷新页面，应该回到第一页，维持每页展现的记录数
			pageList(1,$("#cluePage").bs_pagination('getOption', 'rowsPerPage'));
		});
		//清空查询信息
		$("#emptyButton").click(function (){
			$("#select-fullname").val("");
			$("#select-company").val("");
			$("#select-phone").val("");
			$("#select-source").val("");
			$("#select-owner").val("");
			$("#select-mphone").val("");
			$("#select-state").val("");
			//刷新页面，应该回到第一页，维持每页展现的记录数
			pageList(1,$("#cluePage").bs_pagination('getOption', 'rowsPerPage'));
		});

		//全选、全不选操作
		$("#checkAll").click(function (){
			$(".checkbox").prop("checked",this.checked);
		});
		// 动态生成的元素，我们要以on方法的形式来触发事件
		// 语法：
		// 		$(需要绑定元素的有效的外层元素).on(绑定事件的方式,需要绑定的元素的jquery对象,回调函数)
		$("#clueBodys").on("click",$(".checkbox"),function () {
			$("#checkAll").prop("checked",$(".checkbox").length==$(".checkbox:checked").length);
		})


		pageList(1,2);
	});


	//加载页面信息
	function pageList(pageNo,pageSize){
		//将全选的复选框的√干掉
		// $(":checkbox").prop("checked",false);

		//查询数据，页面加载完成执行
		$.ajax({
			url:"/workbench/clue",
			data:{
				"pageNo" : pageNo,
				"pageSize" : pageSize,
				"fullname" : $.trim($("#select-fullname").val()),
				"company" : $.trim($("#select-company").val()),
				"phone" : $.trim($("#select-phone").val()),
				"source" : $.trim($("#select-source").val()),
				"owner" : $.trim($("#select-owner").val()),
				"mphone" : $.trim($("#select-mphone").val()),
				"state" : $.trim($("#select-state").val())
			},
			type:"GET",
			dataType:"json",
			success:function (data) {
				//表格主体数据
				//每次刷新时，清空之前的数据
				$("#clueBodys").empty();

				var html = "";
				$.each(data.list,function (i,c) {
					html += '<tr>';
					html += '<td><input type="checkbox" class="checkbox"/></td>';
					html += '<td><a style="text-decoration: none; cursor: pointer;" onclick="window.location.href=\'clueDetail\';">'+c.fullname+'</a></td>';
					html += '<td>'+ c.company +'</td>';
					html += '<td>'+ c.phone +'</td>';
					html += '<td>'+ c.mphone+'</td>';
					html += '<td>'+ c.source+'</td>';
					html += '<td>'+ c.owner+'</td>';
					html += '<td>'+ c.state+'</td>';
					html += '</tr>';
				})
				$("#clueBodys").html(html);



				//分页条
				//数据处理完毕后，结合分页查询，对前端展现分页信息
				$("#cluePage").bs_pagination({
					currentPage: pageNo, // 页码
					rowsPerPage: pageSize, // 每页显示的记录条数
					maxRowsPerPage: 20, // 每页最多显示的记录条数
					totalPages: data.pages, // 总页数
					totalRows: data.total, // 总记录条数

					visiblePageLinks: data.navigatePages, // 显示几个卡片

					showGoToPage: true,
					showRowsPerPage: true,
					showRowsInfo: true,
					showRowsDefaultInfo: true,

					//该回调函数时在，点击分页组件的时候触发的
					onChangePage : function(event, data){
						pageList(data.currentPage , data.rowsPerPage);
					}
				});
			}
		});
	};

</script>
